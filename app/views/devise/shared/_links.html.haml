- if controller_name != 'sessions'
  = link_to "ログイン", new_session_path(resource_name), class: 'btn'
  %br/
- if devise_mapping.registerable? && controller_name != 'registrations'
  = link_to "メールアドレスで登録", new_registration_path(resource_name), class: 'btn'
  = link_to 'Googleで登録', user_google_oauth2_omniauth_authorize_path, method: :post, class: 'btn'
  %br/
